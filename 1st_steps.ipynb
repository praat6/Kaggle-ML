    "#Noves variables: \n",
    "    #comunsRef, csRef, ercRef, jxcatRef, ppcRef, pscRef => Binaria; en funció de si fan referència al partit\n",
    "    #numtags => numero de tags sense fer referència a cap dels partits\n",
    "    #...\n",
    "    #La funció \"NewVAr\" s'haurà d'aplicar a la bdd del test per treure les noves variables i utilitzar el model\n",
    "\n",
    "import re #regular expressions\n",
    "\n",
    "def NewVAr(data):\n",
    "    tags=[]\n",
    "    numtags=[]\n",
    "    #Referencias \"Parties\"\n",
    "    comunsRef, csRef, ercRef, jxcatRef, ppcRef, pscRef = [], [], [], [], [], []\n",
    "    i=0\n",
    "    for tweet in data[\"text\"]:\n",
    "        tweetWords = str(tweet).split(sep=\" \")\n",
    "        subtags=[]\n",
    "        comuns, cs, erc, jxcat, ppc, psc = 0, 0, 0, 0, 0, 0\n",
    "        for word in tweetWords:\n",
    "            m = re.search(r\"@\\w+\", word)\n",
    "            if m!=None:\n",
    "                m=m.group()\n",
    "                if m in (\"@EnComu_Podem\",\"ahorapodemos\"):\n",
    "                    coumuns = max(comuns,1)\n",
    "                elif m in (\"@CiudadanosCs\"):\n",
    "                    cs = max(cs,1)\n",
    "                elif m in (\"@Esquerra_ERC\"):\n",
    "                    erc = max(erc,1)\n",
    "                elif m in (\"@JuntsXCat\"):\n",
    "                    jxcat = max(jxcat,1)\n",
    "                elif m in (\"@PPCatalunya\",\"@PPopular\"):\n",
    "                    ppc = max(ppc,1)\n",
    "                elif m in (\"@PSOE\",\"@socialistes_cat\"):\n",
    "                    psc = max(psc,1)\n",
    "                else:\n",
    "                    subtags.append(m)    \n",
    "        tags.append(subtags)\n",
    "        numtags.append(len(subtags))\n",
    "        comunsRef.append(comuns) \n",
    "        csRef.append(cs)\n",
    "        ercRef.append(erc)\n",
    "        jxcatRef.append(jxcat)\n",
    "        ppcRef.append(ppc)\n",
    "        pscRef.append(psc)\n",
    "        i+=1\n",
    "\n",
    "    #Afegir les noves variables\n",
    "    data[\"tags\"] = tags\n",
    "    data[\"numtags\"] = numtags\n",
    "    data[\"comunsRef\"] = comunsRef\n",
    "    data[\"csRef\"] = csRef\n",
    "    data[\"ercRef\"] = ercRef\n",
    "    data[\"jxcatRef\"] = jxcatRef\n",
    "    data[\"ppcRef\"] = ppcRef\n",
    "    data[\"pscRef\"] = pscRef\n",
    "    \n",
    "    return data"
   ]
  },
